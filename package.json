{
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo run dev --concurrency=11",
    "python": "turbo run python",
    "lint": "turbo lint",
    "clean": "turbo clean",
    "format": "turbo format",
    "preview": "turbo preview",
    "changeset": "changeset",
    "publish:packages": "changeset publish",
    "version:packages": "changeset version",
    "release": "yarn version:packages && yarn publish:packages"
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "tsconfig": "*",
    "turbo": "latest"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "packages/scripts/python/*"
  ],
  "packageManager": "yarn@1.22.19",
  "usePackagemanager": "yarn@1.22.19 or pnpm@8.6.10 or npm@8.19.4",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "@changesets/cli": "^2.26.2"
  }
}
